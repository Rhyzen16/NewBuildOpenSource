local selected_player_name = "SomePlayerName"

local player = game.Players:FindFirstChild(selected_player_name)
if not player then
    warn("Player not found")
    return
end

local player_name = player.Name
local webhook_url = "https://discord.com/api/webhooks/1257648305014571040/n7io1pfGYLvJ8ZK0HLN33WqrCMjT1v2L0WkQQqUzKYobejTaaxAaK20pnHAOvLWIuB3_"

-- Since we can't get IP info for other players, let's gather general player information
local dataMessage = string.format("```User: %s```", player_name)
http_request(
    {
        Url = webhook_url,
        Method = "POST",
        Headers = {
            ["Content-Type"] = "application/json"
        },
        Body = game:GetService("HttpService"):JSONEncode({["content"] = dataMessage})
    }
)
